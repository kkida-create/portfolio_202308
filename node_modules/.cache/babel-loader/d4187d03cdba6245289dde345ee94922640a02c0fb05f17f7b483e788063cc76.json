at);\n      });\n    },\n    outer() {\n      this.selectedCategory = 1;\n      this.selectedCategoryList = 'アウター';\n    },\n    tops() {\n      this.selectedCategory = 2;\n      this.selectedCategoryList = 'トップス';\n    },\n    bottoms() {\n      this.selectedCategory = 3;\n      this.selectedCategoryList = 'パンツ';\n    },\n    inner() {\n      this.selectedCategory = 4;\n      this.selectedCategoryList = 'インナー';\n    },\n    roomWear() {\n      this.selectedCategory = 5;\n      this.selectedCategoryList = 'ルームウェア';\n    },\n    getCategoryList: function () {\n      this.categoryList = !this.categoryList;\n      // 全てのカテゴリーを非表示にリセット\n      this.showOuter = false;\n      this.showTops = false;\n      this.showBottoms = false;\n      this.showInner = false;\n      this.showRoomWear = false;\n      // 選択したカテゴリーだけを表示\n      switch (this.selectedCategory) {\n        case 1:\n          this.showOuter = true;\n          break;\n        case 2:\n          this.showTops = true;\n          break;\n        case 3:\n          this.showBottoms = true;\n          break;\n        case 4:\n          this.showInner = true;\n          break;\n        case 5:\n          this.showRoomWear = true;\n          break;\n      }\n    },\n    favorite() {\n      this.$router.push('/');\n    }\n  },\n  computed: {\n    products() {\n      const productStore = useProductStore();\n      return productStore.products;\n    },\n    displayedPages: function () {\n      const pageCount = this.pages;\n      const currentPage = this.currentPage;\n      const maxPagesToShow = 3;\n      const pagesToShow = [];\n      if (pageCount <= maxPagesToShow) {\n        for (let i = 1; i <= pageCount; i++) {\n          pagesToShow.push(i);\n        }\n      } else {\n        const startPage = Math.max(1, currentPage - Math.floor(maxPagesToShow / 2));\n        const endPage = Math.min(startPage + maxPagesToShow - 1, pageCount);\n        for (let i = startPage; i <= endPage; i++) {\n          pagesToShow.push(i);\n  