ntity)\n      console.log('Cart after adding product: ', cartStore.cart)\n      selectedQuantities.value[productId] = 1\n      isModalVisible.value = true\n    }\n    function availableQuantity(product) {\n      const productInCart = cartStore.cart.find((item) => item.product_id === product.product_id)\n      const quantityInCart = productInCart ? productInCart.selectedQuantity : 0\n      return Array.from({ length: product.stock - quantityInCart }, (_, i) => i + 1)\n    }\n    function closeModal() {\n      isModalVisible.value = false\n      this.$router.push('/productDetails')\n    }\n    onMounted(() => {\n      selectedQuantities.value[merchandiseStore.selectedProduct.product_id] = 1\n    })\n    watch(merchandiseStore.selectedProduct, (newProduct) => {\n      productQuantities.splice(\n        0,\n        productQuantities.length,\n        ...Array.from({ length: newProduct.quantity }, (_, index) => index + 1)\n      )\n    })\n    return {\n      selectedProduct: merchandiseStore.selectedProduct,\n      productQuantities,\n      selectedQuantities,\n      addToCart,\n      closeModal,\n      availableQuantity,\n    }\n  },\n}\n</script>\n<style>\n.dropdown-option {\n  background-color: #eeeeee;\n  width: 30%;\n  height: 40px;\n}\n.stock-dropdown-option {\n  background-color: #eeeeee;\n  width: 60%;\n  height: 40px;\n}\n.product-details {\n  background-color: #eeeeee;\n  color: #000000;\n  padding: 30px 0;\n}\n.product-details-warp {\n  display: flex;\n  justify-content: space-between;\n}\n.size-warp,\n.stock-warp {\n  display: flex;\n}\n.img-warp,\n.explanatory-text-warp {\n  width: 50%;\n}\n.explanatory-text-warp {\n  padding-top: 40px;\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  flex-direction: column;\n}\n.img-warp {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.img {\n  width: 70%;\n  border: 1px solid #000000;\n  height: 400px;\n}\n.product-image {\n  width: 100%;\n}\n.product-price,\n.product-tittle {\n  font-size: 24px;\n}\n.explanatory-text {\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n}\n.select-text {\n  margin: 0 20px 0 0;\n  display: flex;\n  align-items: center;\n}\n.size-warp {\n  margin-bottom: 20px;\n}\n.select-warp,\n.button-warp {\n  width: 100%;\n}\n.cart-button {\n  border: 1px solid #000000;\n  width: 70%;\n  height: 50px;\n  font-size: 24px;\n}\n.favorite-wrap {\n  display: flex;\n}\n.favorite {\n  display: flex;\n  width: 30%;\n}\n.stock-warp {\n  width: 50%;\n}\n.modal {\n  background-color: rgb(0 0 0 / 40%);\n  display: block;\n  height: 100%;\n  left: 0;\n  overflow: auto;\n  position: fixed;\n  top: 0;\n  width: 100%;\n  z-index: 1;\n}\n.modal-content {\n  text-align: center;\n  background-color: #fefefe;\n  border: 1px solid #d3d3d3;\n  border-radius: 25px;\n  height: 300px;\n  margin: 15% auto;\n  max-width: 500px;\n  padding: 20px;\n  width: 600px;\n}\n.close {\n  color: #333;\n  font-size: 28px;\n  font-weight: bold;\n  margin-left: 90%;\n}\n.close:hover {\n  color: #000;\n  cursor: pointer;\n  text-decoration: none;\n}\n.heart::before {\n  border-right: none;\n  left: 50%;\n  transform: rotate(-45deg);\n}\n.heart::after {\n  border-left: none;\n  right: 10%;\n  transform: rotate(45deg);\n}\n.favorite.pink::before,\n.favorite.pink::after {\n  background-color: #000;\n}\n.favorite-button {\n  display: flex;\n  position: relative;\n}\n.favorite-wrap {\n  display: block;\n  height: fit-content;\n}\n.heart::before {\n  border-left: none;\n  right: 10%;\n  transform: rotate(45deg);\n}\n.heart::after {\n  border-right: none;\n  left: 20%;\n  transform: rotate(-45deg);\n}\n.heart {\n  border: none;\n  cursor: pointer;\n  height: 30px;\n  width: 30px;\n}\n.heart::before,\n.heart::after {\n  border: 2px solid #000;\n  border-bottom: none;\n  border-radius: 20px 20px 0 0;\n  box-sizing: border-box;\n  content: '';\n  display: block;\n  height: 42px;\n  position: absolute;\n  width: 30px;\n}\n.favorite-wrap {\n  display: flex;\n}\n.favorite-border {\n  display: flex;\n  border: 1px solid #000;\n  height: 50px;\n  width: 55px;\n}\n</style>\n"],"mappings":";;EACOA,EAAE,EAAC,gBAAgB;EAACC,KAAK,EAAC;;;;EACxBA,KAAK,EAAC;;;EACJA,KAAK,EAAC;AAAU;;EACdA,KAAK,EAAC;AAAK;;;EAIbA,KAAK,EAAC;AAAuB;;EAC3BA,KAAK,EAAC;AAAkB;;EACxBA,KAAK,EAAC;AAAgB;;EACpBA,KAAK,EAAC;AAAe;;EAEvBA,KAAK,EAAC;AAAa;;EACjBA,KAAK,EAAC;AAAW;iCACpBC,mBAAA,CAA8B;EAA3BD,KAAK,EAAC;AAAa,GAAC,KAAG;;EAClBA,KAAK,EAAC;AAAiB;;;EAI5BA,KAAK,EAAC;AAAY;iCACrBC,mBAAA,CAAmC;EAAhCD,KAAK,EAAC;AAAa,GAAC,KAAQ;;EAM9BA,KAAK,EAAC;AAAa;;EAWvBA,KAAK,EAAC;AAAO;;EACXA,KAAK,EAAC;AAAe;iCAExBC,mBAAA,CAAkB,WAAf,aAAW;;;uBAxCpBC,mBAAA,CA4CM,OA5CNC,UA4CM,GA3CoCC,MAAA,CAAAC,eAAe,I,cAAvDH,mBAAA,CAmCM,OAnCNI,UAmCM,GAlCJL,mBAAA,CAIM,OAJNM,UAIM,GAHJN,mBAAA,CAEM,OAFNO,UAEM,GADJP,mBAAA,CAA6E;IAAvEQ,GAAG,EAAEL,MAAA,CAAAC,eAAe,CAACK,KAAK;IAAEC,GAAG,EAAC,cAAc;IAACX,KAAK,EAAC;2CAG/DC,mBAAA,CA4BM,OA5BNW,UA4BM,GA3BJX,mBAAA,CAGM,OAHNY,UAGM,GAFJZ,mBAAA,CAAgE,KAAhEa,UAAgE,EAAAC,gBAAA,CAAnCX,MAAA,CAAAC,eAAe,CAACW,YAAY,kBACzDf,mBAAA,CAAqE,OAArEgB,UAAqE,EAA1C,GAAK,GAAAF,gBAAA,CAAGX,MAAA,CAAAC,eAAe,CAACa,KAAK,IAAG,MAAI,gB,GAEjEjB,mBAAA,CAaM,OAbNkB,WAaM,GAZJlB,mBAAA,CAKM,OALNmB,WAKM,GAJJC,WAA8B,EAC9BpB,mBAAA,CAES,UAFTqB,WAES,I,kBADPpB,mBAAA,CAA2EqB,SAAA,QAAAC,WAAA,CAApDC,KAAA,CAAAC,KAAK,EAAbC,IAAI;yBAAnBzB,mBAAA,CAA2E;MAA5C0B,KAAK,EAAED,IAAI;MAAGE,GAAG,EAAEF;wBAASA,IAAI,wBAAAG,WAAA;sCAGnE7B,mBAAA,CAKM,OALN8B,WAKM,GAJJC,WAAmC,E,gBACnC/B,mBAAA,CAES;IAFDD,KAAK,EAAC,uBAAuB;+DAAUI,MAAA,CAAA6B,kBAAkB,CAAC7B,MAAA,CAAAC,eAAe,CAAC6B,UAAU,IAAAC,MAAA;yBAC1FjC,mBAAA,CAAuFqB,SAAA,QAAAC,WAAA,CAAjEpB,MAAA,CAAAgC,iBAAiB,CAAChC,MAAA,CAAAC,eAAe,GAAxCgC,GAAG;yBAAlBnC,mBAAA,CAAuF;MAA5B2B,GAAG,EAAEQ;IAAG,GAAAtB,gBAAA,CAAKsB,GAAG;2EAD9BjC,MAAA,CAAA6B,kBAAkB,CAAC7B,MAAA,CAAAC,eAAe,CAAC6B,UAAU,G,OAKhGjC,mBAAA,CAQM,OARNqC,WAQM,GAPJrC,mBAAA,CAMS;IALPF,EAAE,EAAC,aAAa;IACfwC,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAL,MAAA,IAAE/B,MAAA,CAAAqC,SAAS,CAACrC,MAAA,CAAAC,eAAe,EAAED,MAAA,CAAA6B,kBAAkB,CAAC7B