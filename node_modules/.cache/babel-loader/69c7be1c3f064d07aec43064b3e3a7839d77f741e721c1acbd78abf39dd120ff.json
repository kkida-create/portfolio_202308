{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useProductStore } from '@/store/product';\nconst productStore = useProductStore();\nimport { useMerchandiseStore } from '@/store/merchandise.js';\nexport default {\n  data() {\n    return {\n      slideImage: [require('../assets/slide_1.jpg'), require('../assets/slide_2.jpg'), require('../assets/slide_3.jpg')],\n      countIndex: 0,\n      slideContent: null,\n      productStore\n    };\n  },\n  mounted() {\n    this.startSlide();\n    productStore.fetchProducts();\n  },\n  computed: {\n    firstCountItems() {\n      const filterItems = productStore.products.filter(value => value.is_featured == 1);\n      const sortItems = filterItems.sort((a, b) => b.rating - a.rating);\n      return sortItems.slice(0, 3);\n    },\n    secondCountItems() {\n      const filterItems = productStore.products.filter(value => value.stock);\n      const sortItems = filterItems.sort((a, b) => b.stock - a.stock);\n      return sortItems.slice(0, 4);\n    }\n  },\n  methods: {\n    startSlide() {\n      this.slideContent = setInterval(() => {\n        this.countIndex = (this.countIndex + 1) % this.slideImage.length;\n      }, 5000);\n    },\n    beforeChanging() {\n      clearInterval(this.slideContent);\n    },\n    CatalogDetails(item) {\n      const merchandiseStore = useMerchandiseStore();\n      merchandiseStore.selectProduct({\n        ...item\n      });\n      this.$router.push({\n        name: 'productDetails',\n        params: {\n          product_id: item.product_id\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["useProductStore","productStore","useMerchandiseStore","data","slideImage","require","countIndex","slideContent","mounted","startSlide","fetchProducts","computed","firstCountItems","filterItems","products","filter","value","is_featured","sortItems","sort","a","b","rating","slice","secondCountItems","stock","methods","setInterval","length","beforeChanging","clearInterval","CatalogDetails","item","merchandiseStore","selectProduct","$router","push","name","params","product_id"],"sources":["/Users/kitakanta/Desktop/comvace関係/comvace6_1/src/views/TopView.vue"],"sourcesContent":["<template>\n  <main>\n    <div class=\"all-contents-wrapper\">\n      <div class=\"slide-contents\">\n        <img class=\"slide-image\" :src=\"slideImage[countIndex]\" alt=\"写真\" :key=\"countIndex\" />\n      </div>\n      <div class=\"trend-contents section\">\n        <h2 class=\"trend-contents-title\">2023 SUMMER SELECT COLLECTION</h2>\n        <ul class=\"trend-container\">\n          <li class=\"trend-box\" v-for=\"item in firstCountItems\" :key=\"item.id\">\n            <div class=\"trend-image-box\" @click=\"CatalogDetails(item)\">\n              <img class=\"trend-image\" src=\"../assets/product_sample.jpg\" alt=\"写真\" />\n            </div>\n            <p class=\"inner-text\">{{ item.product_name }}</p>\n            <p class=\"inner-text\">{{ item.price.toLocaleString() }}円</p>\n            <p class=\"inner-text\">{{ item.description }}</p>\n          </li>\n        </ul>\n      </div>\n      <div class=\"sale-contents section\">\n        <h2 class=\"sale-contents-title\">SALE ITEMS</h2>\n        <ul class=\"sale-container\">\n          <li class=\"sale-box\" v-for=\"item in secondCountItems\" :key=\"item.id\">\n            <router-link class=\"sale-image-box\" to=\"/productDetails\" @click=\"CatalogDetails(item)\">\n              <img class=\"sale-image\" src=\"../assets/product_sample.jpg\" alt=\"写真\" />\n            </router-link>\n            <p class=\"inner-text\">{{ item.product_name }}</p>\n            <p class=\"inner-text\">{{ item.special_price.toLocaleString() }}円</p>\n          </li>\n        </ul>\n      </div>\n      <div class=\"products-button\">\n        <router-link class=\"products-button-link\" to=\"/product\">\n          <img class=\"products-search-icon\" src=\"../assets/icon_search.png\" alt=\"アイコン\" />\n          <p class=\"products-search-text\">商品を探す</p>\n        </router-link>\n      </div>\n    </div>\n  </main>\n</template>\n\n<script>\nimport { useProductStore } from '@/store/product'\nconst productStore = useProductStore()\nimport { useMerchandiseStore } from '@/store/merchandise.js'\nexport default {\n  data() {\n    return {\n      slideImage: [\n        require('../assets/slide_1.jpg'),\n        require('../assets/slide_2.jpg'),\n        require('../assets/slide_3.jpg'),\n      ],\n      countIndex: 0,\n      slideContent: null,\n      productStore,\n    }\n  },\n  mounted() {\n    this.startSlide()\n    productStore.fetchProducts()\n  },\n  computed: {\n    firstCountItems() {\n      const filterItems = productStore.products.filter((value) => value.is_featured == 1)\n      const sortItems = filterItems.sort((a, b) => b.rating - a.rating)\n      return sortItems.slice(0, 3)\n    },\n    secondCountItems() {\n      const filterItems = productStore.products.filter((value) => value.stock)\n      const sortItems = filterItems.sort((a, b) => b.stock - a.stock)\n      return sortItems.slice(0, 4)\n    },\n  },\n  methods: {\n    startSlide() {\n      this.slideContent = setInterval(() => {\n        this.countIndex = (this.countIndex + 1) % this.slideImage.length\n      }, 5000)\n    },\n    beforeChanging() {\n      clearInterval(this.slideContent)\n    },\n    CatalogDetails(item) {\n      const merchandiseStore = useMerchandiseStore()\n      merchandiseStore.selectProduct({ ...item })\n      this.$router.push({\n        name: 'productDetails',\n        params: {\n          product_id: item.product_id,\n        },\n      })\n    },\n  },\n}\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=Noto+Serif:ital@1&display=swap');\n.all-contents-wrapper {\n  background-color: #eeeeee;\n  padding-bottom: 120px;\n}\n.slide-contents {\n  width: 100vw;\n  height: 100vh;\n}\n.slide-image {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  animation-name: fadeInSlide;\n  animation-duration: 5s;\n  animation-fill-mode: forwards;\n  opacity: 0;\n}\n@keyframes fadeInSlide {\n  from {\n    opacity: 0.2;\n  }\n  to {\n    opacity: 0.8;\n  }\n}\n.section {\n  margin-top: 200px;\n}\n.trend-contents-title {\n  text-align: center;\n  font-size: 40px;\n}\n.trend-container {\n  display: flex;\n  justify-content: center;\n  list-style: none;\n  margin-top: 40px;\n  padding-left: 0;\n}\n.trend-box {\n  width: 30%;\n}\n.trend-box + .trend-box {\n  margin-left: 20px;\n}\n.trend-image-box {\n  display: block;\n  background-color: #ffffff;\n  height: auto;\n  aspect-ratio: 4 / 3;\n  text-align: center;\n}\n.trend-image {\n  width: auto;\n  height: 100%;\n}\n.inner-text {\n  font-size: 18px;\n  padding: 0 20px;\n}\n.inner-text:first-of-type {\n  margin-top: 20px;\n}\n.trend-box .inner-text:last-of-type {\n  font-size: 14px;\n}\n.sale-contents {\n  padding: 0 10%;\n}\n.sale-contents-title {\n  font-size: 40px;\n  font-family: 'Noto Serif', serif;\n}\n.sale-container {\n  display: flex;\n  justify-content: center;\n  list-style: none;\n  margin-top: 40px;\n  padding-left: 0;\n}\n.sale-box + .sale-box {\n  margin-left: 20px;\n}\n.sale-image-box {\n  display: block;\n  height: auto;\n  aspect-ratio: 1 / 1;\n  text-align: center;\n  border-radius: 40px;\n  position: relative;\n}\n.sale-image-box::before {\n  content: 'SALE';\n  color: #ffffff;\n  position: absolute;\n  background-color: #ff0000;\n  padding: 4px 12px;\n  border-radius: 20px;\n  top: 12px;\n  right: 12px;\n  border: 2px solid #ffffff;\n}\n.sale-image {\n  width: 100%;\n  height: auto;\n  border-radius: 40px;\n}\n.sale-box .inner-text:last-of-type {\n  color: #ff0000;\n}\n.products-button {\n  display: flex;\n  justify-content: center;\n  margin-top: 120px;\n}\n.products-button-link {\n  text-decoration: none;\n  color: #000000;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 320px;\n  height: 60px;\n  border: 1px solid #000000;\n}\n.products-search-icon {\n  width: 32px;\n}\n.products-search-text {\n  margin-bottom: 0;\n  font-size: 24px;\n  margin-left: 20px;\n}\n.trend-image-box:hover,\n.sale-image-box:hover,\n.products-button-link:hover {\n  box-shadow: 0 0 12px #a9a9a9;\n  background-color: #ffffff;\n}\n</style>\n"],"mappings":";AA0CA,SAASA,eAAc,QAAS,iBAAgB;AAChD,MAAMC,YAAW,GAAID,eAAe,CAAC;AACrC,SAASE,mBAAkB,QAAS,wBAAuB;AAC3D,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,CACVC,OAAO,CAAC,uBAAuB,CAAC,EAChCA,OAAO,CAAC,uBAAuB,CAAC,EAChCA,OAAO,CAAC,uBAAuB,CAAC,CACjC;MACDC,UAAU,EAAE,CAAC;MACbC,YAAY,EAAE,IAAI;MAClBN;IACF;EACF,CAAC;EACDO,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC;IAChBR,YAAY,CAACS,aAAa,CAAC;EAC7B,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,MAAMC,WAAU,GAAIZ,YAAY,CAACa,QAAQ,CAACC,MAAM,CAAEC,KAAK,IAAKA,KAAK,CAACC,WAAU,IAAK,CAAC;MAClF,MAAMC,SAAQ,GAAIL,WAAW,CAACM,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACC,MAAK,GAAIF,CAAC,CAACE,MAAM;MAChE,OAAOJ,SAAS,CAACK,KAAK,CAAC,CAAC,EAAE,CAAC;IAC7B,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB,MAAMX,WAAU,GAAIZ,YAAY,CAACa,QAAQ,CAACC,MAAM,CAAEC,KAAK,IAAKA,KAAK,CAACS,KAAK;MACvE,MAAMP,SAAQ,GAAIL,WAAW,CAACM,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACI,KAAI,GAAIL,CAAC,CAACK,KAAK;MAC9D,OAAOP,SAAS,CAACK,KAAK,CAAC,CAAC,EAAE,CAAC;IAC7B;EACF,CAAC;EACDG,OAAO,EAAE;IACPjB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,YAAW,GAAIoB,WAAW,CAAC,MAAM;QACpC,IAAI,CAACrB,UAAS,GAAI,CAAC,IAAI,CAACA,UAAS,GAAI,CAAC,IAAI,IAAI,CAACF,UAAU,CAACwB,MAAK;MACjE,CAAC,EAAE,IAAI;IACT,CAAC;IACDC,cAAcA,CAAA,EAAG;MACfC,aAAa,CAAC,IAAI,CAACvB,YAAY;IACjC,CAAC;IACDwB,cAAcA,CAACC,IAAI,EAAE;MACnB,MAAMC,gBAAe,GAAI/B,mBAAmB,CAAC;MAC7C+B,gBAAgB,CAACC,aAAa,CAAC;QAAE,GAAGF;MAAK,CAAC;MAC1C,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI,EAAE,gBAAgB;QACtBC,MAAM,EAAE;UACNC,UAAU,EAAEP,IAAI,CAACO;QACnB;MACF,CAAC;IACH;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}