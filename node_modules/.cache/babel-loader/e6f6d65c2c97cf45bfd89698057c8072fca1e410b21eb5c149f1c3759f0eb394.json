ace6_1/src/views/ProductDetailsView.vue"],"sourcesContent":["<template>\n  <div id=\"productDetails\" class=\"product-details\">\n    <div class=\"product-details-warp\" v-if=\"selectedProduct\">\n      <div class=\"img-warp\">\n        <div class=\"img\">\n          <img :src=\"selectedProduct.image\" alt=\"ProductImage\" class=\"product-image\" />\n        </div>\n      </div>\n      <div class=\"explanatory-text-warp\">\n        <div class=\"explanatory-text\">\n          <p class=\"product-tittle\">{{ selectedProduct.product_name }}</p>\n          <div class=\"product-price\">&yen;{{ selectedProduct.price }}(税込)</div>\n        </div>\n        <div class=\"select-warp\">\n          <div class=\"size-warp\">\n            <p class=\"select-text\">サイズ</p>\n            <select class=\"dropdown-option\">\n              <option v-for=\"size in sizes\" :value=\"size\" :key=\"size\">{{ size }}</option>\n            </select>\n          </div>\n          <div class=\"favorite-wrap\">\n            <div class=\"stock-warp\">\n              <p class=\"select-text\">数量&emsp;</p>\n              <select class=\"stock-dropdown-option\" v-model=\"selectedQuantities[selectedProduct.product_id]\">\n                <option v-for=\"num in availableQuantity(selectedProduct)\" :key=\"num\">{{ num }}</option>\n              </select>\n            </div>\n            <div class=\"favorite\">\n              <div class=\"favorite-button\" :class=\"{ favorite: !isFavorite }\"></div>\n              <div class=\"favorite-border\">\n                <button\n                  :class=\"['heart', { 'favorite pink': isFavorite }, { favorite: !isFavorite }, 'favorite-button']\"\n                  @click=\"toggleFavorite(product, favoriteQuantities[product.product_id])\"\n                ></button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"button-warp\">\n          <button\n            id=\"add-to-cart\"\n            @click=\"addToCart(selectedProduct, selectedQuantities[selectedProduct.product_id])\"\n            class=\"cart-button\"\n          >\n            カートに追加\n          </button>\n        </div>\n      </div>\n    </div>\n    <div class=\"modal\" v-show=\"isModalVisible\">\n      <div class=\"modal-content\">\n        <span class=\"close\" @click=\"closeModal\">&times;</span>\n        <p>カートに追加しました。</p>\n        <button class=\"cart\" @click=\"Cartpage\">カートへ</button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { useMerchandiseStore } from '@/store/merchandise.js'\nimport { reactive, watch, ref onMounted } from 'vue'\nimport { useCartStore } from '@/store/cart.js'\nimport { useFavoriteStore } from '@/store/favorite.js'\nexport default {\n  name: 'productDetails',\n  data() {\n    return {\n      sizes: ['S', 'M', 'L'],\n      isFavorite: false,\n      isModalVisible: false,\n      quantity: 0,\n    }\n  },\n  setup() {\n    const cartStore = useCartStore()\n    const merchandiseStore = useMerchandiseStore()\n    const favoriteStore = useFavoriteStore()\n    const productQuantities = reactive([])\n    const selectedQuantities = ref({})\n    const favoriteQuantities = ref(1)\n    function addToCart(product, quantity) {\n      const productId = product?.product_id\n      if (!productId) return\n      cartStore.addToCart(productId, quantity)\n      selectedQuantities.value[productId] = 1 // Add to cart and then reset to 1\n      this.isModalVisible = true\n    }\n    function availableQuantity(product) {\n      const productInCart = cartStore.cart.find((item) => item.product_id === product.product_id)\n      const quantityInCart = productInCart ? productInCart.selectedQuantity : 0\n      return Array.from({ length: product.stock - quantityInCart }, (_, i) => i + 1)\n    }\n    function closeModal() {\n      this.isModalVisible = false\n      this.$router.push('/productDetails')\n    }\n    onMounted(() => {\n      selectedQuantities.value[merchandiseStore.selectedProduct.product_id] = 1\n    })\n    function toggleFavorite() {\n      this.isFavorite = !this.isFavorite\n      favoriteStore.setFavoriteQuantities(favoriteQuantities.value)\n      this.$router.push('/favorite')\n    }\n    watch(merchandiseStore.selectedProduct, (newProduct) => {\n      productQuantities.splice(\n        0,\n        productQuantities.length,\n        ...Array.from({ length: newProduct.quantity }, (_, index) => index + 1)\n      )\n    })\n    return {\n      selectedProduct: merchandiseStore.selectedProduct,\n      productQuantities,\n      selectedQuantities,\n      favoriteQuantities,\n      addToCart,\n      closeModal,\n      toggleFavorite,\n      availableQuantity,\n    }\n  },\n  computed: {\n    computedProduct() {\n      const merchandiseStore = useMerchandiseStore()\n      return merchandiseStore.selectedProduct\n    },\n  },\n  methods: {\n    //addToCart() {\n    //if (!this.isAuthenticated) {\n    // ログイン状態でない場合、ログイン画面へ遷移する\n    //this.$router.push('/login')\n    //return\n    //}\n    //const merchandiseStore = useMerchandiseStore()\n    //merchandiseStore.selectProduct(product)\n    //this.isModalVisible = true\n    //},\n    //closeModal() {\n    //this.isModalVisible = false\n    //this.$router.push('/productDetails')\n    //},\n  },\n}\n</script>\n<style>\n.dropdown-option {\n  background-color: #eeeeee;\n  width: 30%;\n  height: 40px;\n}\n.stock-dropdown-option {\n  background-color: #eeeeee;\n  width: 60%;\n  height: 40px;\n}\n.product-details {\n  background-color: #eeeeee;\n  color: #000000;\n  padding: 30px 0;\n}\n.product-details-warp {\n  display: flex;\n  justify-content: space-between;\n}\n.size-warp,\n.stock-warp {\n  display: flex;\n}\n.img-warp,\n.explanatory-text-warp {\n  width: 50%;\n}\n.explanatory-text-warp {\n  padding-top: 40px;\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  flex-direction: column;\n}\n.img-warp {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.img {\n  width: 70%;\n  border: 1px solid #000000;\n  height: 400px;\n}\n.product-image {\n  width: 100%;\n}\n.product-price,\n.product-tittle {\n  font-size: 24px;\n}\n.explanatory-text {\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n}\n.select-text {\n  margin: 0 20px 0 0;\n  display: flex;\n  align-items: center;\n}\n.size-warp {\n  margin-bottom: 20px;\n}\n.select-warp,\n.button-warp {\n  width: 100%;\n}\n.cart-button {\n  border: 1px solid #000000;\n  width: 70%;\n  height: 50px;\n  font-size: 24px;\n}\n.favorite-wrap {\n  display: flex;\n}\n.favorite {\n  display: flex;\n  width: 30%;\n}\n.stock-warp {\n  width: 50%;\n}\n.modal {\n  background-color: rgb(0 0 0 / 40%);\n  display: block;\n  height: 100%;\n  left: 0;\n  overflow: auto;\n  position: fixed;\n  top: 0;\n  width: 100%;\n  z-index: 1;\n}\n.modal-content {\n  text-align: center;\n  background-color: #fefefe;\n  border: 1px solid #d3d3d3;\n  border-radius: 25px;\n  height: 300px;\n  margin: 15% auto;\n  max-width: 500px;\n  padding: 20px;\n  width: 600px;\n}\n.close {\n  color: #333;\n  font-size: 28px;\n  font-weight: bold;\n  margin-left: 90%;\n}\n.close:hover {\n  color: #000;\n  cursor: pointer;\n  text-decoration: none;\n}\n.heart::before {\n  border-right: none;\n  left: 50%;\n  transform: rotate(-45deg);\n}\n.heart::after {\n  border-left: none;\n  right: 10%;\n  transform: rotate(45deg);\n}\n.favorite.pink::before,\n.favorite.pink::after {\n  background-color: #000;\n}\n.favorite-button {\n  display: flex;\n  position: relative;\n}\n.favorite-wrap {\n  display: block;\n  height: fit-content;\n}\n.heart::before {\n  border-left: none;\n  right: 10%;\n  transform: rotate(45deg);\n}\n.heart::after {\n  border-right: none;\n  left: 20%;\n  transform: rotate(-45deg);\n}\n.heart {\n  border: none;\n  cursor: pointer;\n  height: 30px;\n  width: 30px;\n}\n.heart::before,\n.heart::after {\n  border: 2px solid #000;\n  border-bottom: none;\n  border-radius: 20px 20px 0 0;\n  