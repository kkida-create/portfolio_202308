{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useFavoriteStore } from \"@/store/favorite.js\";\nimport { useCartStore } from \"@/store/cart.js\";\nimport { useRouter } from \"vue-router\";\nimport { ref } from \"vue\";\nimport { onMounted } from \"vue\";\nimport { useUserStore } from \"@/store/user.js\";\nexport default {\n  data: function data() {\n    return {};\n  },\n  setup: function setup() {\n    onMounted(function () {\n      window.scrollTo(0, 0);\n    });\n    var favoriteStore = useFavoriteStore();\n    var cartStore = useCartStore();\n    var favoriteQuantities = ref({});\n    var router = useRouter();\n    var userStore = useUserStore();\n    var isLoginErrorVisible = ref(false);\n    function liftFavorite(product) {\n      favoriteStore.liftFavoriteQuantities(product);\n    }\n    function addToCart(product, quantity) {\n      if (!userStore.userId) {\n        isLoginErrorVisible.value = true;\n        return;\n      }\n      var productId = product.product_id;\n      if (!productId) return;\n      cartStore.addToCart(productId, quantity);\n      favoriteQuantities.value[productId] = 1;\n      router.push(\"/cart-list\");\n    }\n    return {\n      favoriteQuantities: favoriteStore.favoriteQuantities,\n      liftFavorite: liftFavorite,\n      addToCart: addToCart,\n      isLoginErrorVisible: isLoginErrorVisible\n    };\n  }\n};","map":{"version":3,"names":["useFavoriteStore","useCartStore","useRouter","ref","onMounted","useUserStore","data","setup","window","scrollTo","favoriteStore","cartStore","favoriteQuantities","router","userStore","isLoginErrorVisible","liftFavorite","product","liftFavoriteQuantities","addToCart","quantity","userId","value","productId","product_id","push"],"sources":["/Users/kitakanta/Desktop/就活/ポートフォリオ_20230819/src/views/FavoriteView.vue"],"sourcesContent":["<template>\n  <div class=\"favorite-warp\">\n    <div class=\"favorite-tittle\">\n      <p>お気に入り</p>\n      <div class=\"error-message\" v-if=\"isLoginErrorVisible\">\n        カート機能を使うにはログインしてください。\n      </div>\n    </div>\n    <div\n      class=\"favorite-product-warp\"\n      v-for=\"product in favoriteQuantities\"\n      :key=\"product.product_id\"\n    >\n      <div class=\"favorite-img-warp\">\n        <div class=\"favorite-img\">\n          <img\n            src=\"../assets/product_sample.jpg\"\n            alt=\"ProductImage\"\n            class=\"favorite-product-image\"\n          />\n        </div>\n      </div>\n      <div class=\"favorite-explanatory-text\">\n        <p class=\"favorite-product-tittle\">{{ product.product_name }}</p>\n        <div class=\"favorite-product-price\">&yen;{{ product.price }}(税込)</div>\n      </div>\n      <div class=\"button-warp-second\">\n        <div class=\"cart-button-warp\">\n          <button\n            id=\"favorite-add-to-cart\"\n            @click=\"addToCart(product, 1)\"\n            class=\"favorite-cart-button\"\n          >\n            カートに追加\n          </button>\n        </div>\n        <div class=\"favorite-button-warp\">\n          <button class=\"favorite-button-second\" @click=\"liftFavorite(product)\">\n            お気に入り解除\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { useFavoriteStore } from \"@/store/favorite.js\";\nimport { useCartStore } from \"@/store/cart.js\";\nimport { useRouter } from \"vue-router\";\nimport { ref } from \"vue\";\nimport { onMounted } from \"vue\";\nimport { useUserStore } from \"@/store/user.js\";\n\nexport default {\n  data() {\n    return {};\n  },\n  setup() {\n    onMounted(() => {\n      window.scrollTo(0, 0);\n    });\n    const favoriteStore = useFavoriteStore();\n    const cartStore = useCartStore();\n    const favoriteQuantities = ref({});\n    const router = useRouter();\n    const userStore = useUserStore();\n    const isLoginErrorVisible = ref(false);\n    function liftFavorite(product) {\n      favoriteStore.liftFavoriteQuantities(product);\n    }\n    function addToCart(product, quantity) {\n      if (!userStore.userId) {\n        isLoginErrorVisible.value = true;\n        return;\n      }\n      const productId = product.product_id;\n      if (!productId) return;\n      cartStore.addToCart(productId, quantity);\n      favoriteQuantities.value[productId] = 1;\n      router.push(\"/cart-list\");\n    }\n    return {\n      favoriteQuantities: favoriteStore.favoriteQuantities,\n      liftFavorite,\n      addToCart,\n      isLoginErrorVisible,\n    };\n  },\n};\n</script>\n<style>\n.favorite-warp {\n  padding-top: 116px;\n  background-color: #eeeeee;\n  color: #000000;\n  padding: 130px 30px 30px 30px;\n}\n.favorite-tittle {\n  font-size: 24px;\n}\n.favorite-product-warp {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n  margin-top: 30px;\n  margin-bottom: 30px;\n}\n.favorite-explanatory-text,\n.favorite-img-warp {\n  width: 30%;\n}\n.favorite-explanatory-text {\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n}\n.button-warp-second {\n  width: 40%;\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n.favorite-img-warp {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.favorite-img {\n  width: 70%;\n  border: 1px solid #000000;\n}\n.cart-button-warp,\n.favorite-button-warp {\n  width: 100%;\n}\n.favorite-cart-button,\n.favorite-button-second {\n  width: 50%;\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n  border: 1px solid #000000;\n}\n.favorite-cart-button {\n  background-color: #000000;\n  color: white;\n  margin-bottom: 20px;\n}\n.favorite-product-image {\n  object-fit: cover;\n  width: 100%;\n}\n.error-message {\n  color: red;\n  font-size: 18px;\n}\n</style>\n"],"mappings":";AA8CA,SAASA,gBAAe,QAAS,qBAAqB;AACtD,SAASC,YAAW,QAAS,iBAAiB;AAC9C,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,GAAE,QAAS,KAAK;AACzB,SAASC,SAAQ,QAAS,KAAK;AAC/B,SAASC,