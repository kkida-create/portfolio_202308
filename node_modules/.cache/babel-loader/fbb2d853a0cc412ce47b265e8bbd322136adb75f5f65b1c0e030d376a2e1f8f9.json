{"ast":null,"code":"export default {\n  data() {\n    return {\n      addresses: [{\n        name: 'Option 1',\n        isActive: false,\n        address: '何県何市何丁目'\n      }, {\n        name: 'Option 2',\n        isActive: false,\n        address: '別県別市別丁目'\n      }, {\n        name: 'Option 3',\n        isActive: false,\n        address: 'さらに別県別市別丁目'\n      }],\n      paymentMethods: [{\n        name: 'クレジットカード',\n        description: 'Visa 末尾 xxxx Tanaka xx/xxxx',\n        hasModalButton: true\n      }, {\n        name: '代金引換え',\n        description: '国内配送のみ。代引手数料がかかります。',\n        hasModalButton: false\n      }, {\n        name: 'コンビニ・ATM・ネットバンキング・電子マネー支払い',\n        description: '商品の発送は代金のお支払い後になるため、お届け予定日はお支払い確定後にご案内いたします。',\n        hasModalButton: false\n      }],\n      selectedAddressIndex: null,\n      selectedPaymentMethodIndex: null\n    };\n  },\n  methods: {\n    toggleAddressCircle(index) {\n      this.selectedAddressIndex = index;\n    },\n    togglePaymentMethodCircle(index) {\n      this.selectedPaymentMethodIndex = index;\n    }\n  }\n};","map":{"version":3,"names":["data","addresses","name","isActive","address","paymentMethods","description","hasModalButton","selectedAddressIndex","selectedPaymentMethodIndex","methods","toggleAddressCircle","index","togglePaymentMethodCircle"],"sources":["/Users/kitakanta/Desktop/comvace関係/comvace6_1/src/views/CartPaymentView.vue"],"sourcesContent":["<template>\n  <main>\n    <div class=\"payment-top\">\n      <h1 class=\"page-title\">支払い画面</h1>\n      <div class=\"contents-container\">\n        <div class=\"address-container\">\n          <p class=\"address-title\">1 住所</p>\n          <div class=\"address-select-container\" v-for=\"(address, index) in addresses\" :key=\"index\">\n            <div class=\"outer-circle\" @click=\"toggleAddressCircle(index)\">\n              <div class=\"inner-circle\" :class=\"{ 'inner-circle-selected': selectedAddressIndex === index }\"></div>\n            </div>\n            <div class=\"address-text\">{{ address.address }}</div>\n          </div>\n          <div class=\"address-registration\" @click=\"openModal\">\n            <div class=\"cross\"></div>\n            新しい住所を登録\n          </div>\n        </div>\n        <div class=\"payment-container\">\n          <p class=\"payment-title\">2 お支払い方法</p>\n          <div class=\"payment-info-container\" v-for=\"(method, index) in paymentMethods\" :key=\"index\">\n            <div class=\"outer-circle\" @click=\"togglePaymentMethodCircle(index)\">\n              <div\n                class=\"inner-circle\"\n                :class=\"{ 'inner-circle-selected': selectedPaymentMethodIndex === index }\"\n              ></div>\n            </div>\n            <div class=\"payment-info-contents\">\n              <div class=\"payment-info\">\n                <p class=\"payment-text\">{{ method.name }}</p>\n                <p class=\"payment-content\">{{ method.description }}</p>\n              </div>\n              <button v-if=\"method.hasModalButton\" class=\"credit-modal-open\">クレジットカードの変更</button>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"button-container\">\n        <button class=\"return-to-cart\">← カート一覧へ戻る</button>\n        <button class=\"go-to-cart-check\">ご注文確認へ進む →</button>\n      </div>\n    </div>\n    <div class=\"address-modal\">\n      <div class=\"input-top\">\n        <div class=\"input-message\">新しいお届け先を入力</div>\n        <div class=\"address-close\">×</div>\n        <div class=\"name-container\">\n          <div class=\"name-title\">お名前</div>\n          <input type=\"text\" class=\"first-name\" />\n          <input type=\"text\" class=\"last-name\" />\n        </div>\n        <div class=\"zip-code-container\">\n          <div class=\"zip-code-title\">郵便番号</div>\n          <input type=\"text\" class=\"zip-code\" name=\"zip-code\" placeholder=\"123-4567\" />\n        </div>\n        <div class=\"prefecture-container\">\n          <div class=\"prefecture-title\">都道府県</div>\n          <select class=\"prefecture-pull-down\">\n            <option v-for=\"prefecture in prefectures\" :value=\"prefecture\" id=\"prefecture\" :key=\"prefecture\">\n              {{ prefecture }}\n            </option>\n          </select>\n        </div>\n      </div>\n      <div class=\"input-address\" v-for=\"address in adresses\" id=\"adress\" :key=\"address\">\n        <div class=\"address-title\"></div>\n        <input type=\"text\" class=\"address-input-box\" />\n      </div>\n      <button class=\"address-submit\">この住所を使用</button>\n    </div>\n    <div class=\"credit-modal\">\n      <div class=\"credit-modal-title\">お客様のクレジットカード</div>\n      <div class=\"credit-detail-container\">\n        <span>カード番号: {{ cardNumber }}</span>\n        <span>名義人: {{ cardName }}</span>\n        <span>有効期限: {{ expiryDate }}</span>\n        <span>セキュリティコード: {{ securityCode }}</span>\n      </div>\n      <div class=\"credit-registration\">新しいクレジットカードを追加</div>\n      <div class=\"credit-input-modal\">\n        <div class=\"credit-input-container\" v-for=\"(field, index) in cardFields\" :key=\"index\">\n          <label class=\"credit-label\" :for=\"field.id\">{{ field.label }}:</label>\n          <input class=\"credit-input-box\" :type=\"field.type\" :id=\"field.id\" v-model=\"field.value\" />\n        </div>\n        <button class=\"close-button\" @click=\"closeModal\">×</button>\n        <button class=\"add-credit\" @click=\"submitForm\">カード追加</button>\n      </div>\n    </div>\n  </main>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      addresses: [\n        { name: 'Option 1', isActive: false, address: '何県何市何丁目' },\n        { name: 'Option 2', isActive: false, address: '別県別市別丁目' },\n        { name: 'Option 3', isActive: false, address: 'さらに別県別市別丁目' },\n      ],\n      paymentMethods: [\n        {\n          name: 'クレジットカード',\n          description: 'Visa 末尾 xxxx Tanaka xx/xxxx',\n          hasModalButton: true,\n        },\n        {\n          name: '代金引換え',\n          description: '国内配送のみ。代引手数料がかかります。',\n          hasModalButton: false,\n        },\n        {\n          name: 'コンビニ・ATM・ネットバンキング・電子マネー支払い',\n          description: '商品の発送は代金のお支払い後になるため、お届け予定日はお支払い確定後にご案内いたします。',\n          hasModalButton: false,\n        },\n      ],\n      selectedAddressIndex: null,\n      selectedPaymentMethodIndex: null,\n    }\n  },\n  methods: {\n    toggleAddressCircle(index) {\n      this.selectedAddressIndex = index\n    },\n    togglePaymentMethodCircle(index) {\n      this.selectedPaymentMethodIndex = index\n    },\n  },\n}\n</script>\n\n<style scoped>\n.payment-top {\n  min-width: 950px;\n  background-color: #eeeeee;\n  min-height: 100vh;\n  padding: 30px 20px;\n}\n.page-title {\n  font-size: 40px;\n}\n.contents-container {\n  margin-top: 30px;\n  text-align: left;\n}\n.outer-circle {\n  width: 30px;\n  height: 30px;\n  border: 2px solid black;\n  border-radius: 50%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  cursor: pointer;\n}\n.inner-circle {\n  width: 15px;\n  height: 15px;\n  border-radius: 50%;\n}\n.inner-circle-selected {\n  background-color: black;\n}\n.address-title {\n  font-size: 24px;\n}\n.address-select-container {\n  display: flex;\n  margin-top: 10px;\n}\n.address-text {\n  margin-left: 10px;\n}\n.address-registration {\n  display: flex;\n  align-items: center;\n  padding-left: 5px;\n  margin-top: 10px;\n  display: inline-block;\n}\n.address-registration:hover {\n  color: blue;\n  cursor: pointer;\n}\n.address-registration:hover .cross::before,\n.address-registration:hover .cross::after {\n  background-color: blue;\n}\n.cross {\n  display: inline-block;\n  position: relative;\n  height: 20px;\n  width: 20px;\n  margin-right: 10px;\n}\n\n.cross::before,\n.cross::after {\n  content: '';\n  position: absolute;\n  background: rgba(9, 9, 9, 0.506);\n}\n\n.cross::before {\n  height: 5px;\n  width: 100%;\n  top: 50%;\n  left: 0;\n  transform: translateY(-50%);\n}\n\n.cross::after {\n  width: 5px;\n  height: 100%;\n  left: 50%;\n  top: 0;\n  transform: translateX(-50%);\n}\n.payment-container {\n  margin-top: 30px;\n}\n.payment-title {\n  font-size: 24px;\n}\n.payment-info-container {\n  margin-top: 30px;\n  display: flex;\n  align-items: center;\n  width: 90%;\n}\n\n.payment-info-contents {\n  border: solid 1px black;\n  padding: 14px;\n  margin-left: 10px;\n  width: 100%;\n}\n.payment-info-contents:nth-last-of-type(1) {\n  display: flex;\n  justify-content: space-between;\n}\n.payment-text {\n  font-weight: bold;\n  margin: 0;\n}\n.payment-content {\n  margin: 0;\n}\n.address-modal {\n  display: none;\n}\n.credit-modal {\n  display: none;\n}\n.credit-modal-open {\n  background: none;\n  color: inherit;\n  border: none;\n  padding: 0;\n  font: inherit;\n  cursor: pointer;\n  outline: inherit;\n  color: rgb(0, 102, 255);\n}\n.credit-modal-open:hover {\n  text-decoration: underline;\n}\n</style>\n"],"mappings":"AA4FA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE,KAAK;QAAEC,OAAO,EAAE;MAAU,CAAC,EACzD;QAAEF,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE,KAAK;QAAEC,OAAO,EAAE;MAAU,CAAC,EACzD;QAAEF,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE,KAAK;QAAEC,OAAO,EAAE;MAAa,CAAC,CAC7D;MACDC,cAAc,EAAE,CACd;QACEH,IAAI,EAAE,UAAU;QAChBI,WAAW,EAAE,6BAA6B;QAC1CC,cAAc,EAAE;MAClB,CAAC,EACD;QACEL,IAAI,EAAE,OAAO;QACbI,WAAW,EAAE,qBAAqB;QAClCC,cAAc,EAAE;MAClB,CAAC,EACD;QACEL,IAAI,EAAE,4BAA4B;QAClCI,WAAW,EAAE,8CAA8C;QAC3DC,cAAc,EAAE;MAClB,CAAC,CACF;MACDC,oBAAoB,EAAE,IAAI;MAC1BC,0BAA0B,EAAE;IAC9B;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,mBAAmBA,CAACC,KAAK,EAAE;MACzB,IAAI,CAACJ,oBAAmB,GAAII,KAAI;IAClC,CAAC;IACDC,yBAAyBA,CAACD,KAAK,EAAE;MAC/B,IAAI,CAACH,0BAAyB,GAAIG,KAAI;IACxC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}